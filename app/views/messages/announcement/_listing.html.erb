<h1>Current Message to Employees</h1>
  <% @messages.each do |f| %>
      <div class="field">
        <%= f.title %>
        <%= f.body.html_safe %>
          <%= link_to 'Show', f %></td>
          <%= link_to 'Edit', edit_message_path(f) %>
          <% if admin? %>
          <%= link_to 'Destroy', f, method: :delete, data: { confirm: 'Are you sure?' } %>
          <% end %>
      </div>
    <% end %>
<br>
<%= link_to 'message', new_message_path %>
